Xft.antialias:                 true
Xft.hinting:                   true
Xft.lcdfilter:                 lcddefault

URxvt.termName:                rxvt-256color
URxvt.iso14755:                false
URxvt.iso14755_52:             false
URxvt.saveLines:               5000
URxvt.urgentOnBell:            true
URxvt.visualBell:              false

URxvt.perl-ext-common:         default,matcher,font-size,keyboard-select

URxvt.url-launcher:            chromium

URxvt.keysym.C-S-Up:           font-size:increase
URxvt.keysym.C-S-Down:         font-size:decrease
URxvt.keysym.C-0:              font-size:reset
URxvt.font-size.step:          4

URxvt.keysym.M-Escape:         perl:keyboard-select:activate
URxvt.keysym.M-s:              perl:keyboard-select:search

URxvt.allow_bold:              true
URxvt.font:                    xft:Source Code Pro Medium:size=10, \
                               xft:DejaVu Sans Mono:size=10
URxvt*boldFont:                xft:Source Code Pro for Powerline-10:bold

URxvt.depth:                   32
URxvt.transparent:             false
URxvt.fading:                  1
URxvt.lineSpace:               0
URxvt.pointerBlank:            true
URxvt.borderless:              true
URxvt.scrollStyle:             rxvt
URxvt.scrollBar:               false
URxvt.cursorBlink:             false
URxvt.cursorColor:             #657b83
URxvt.cursorUnderline:         false
URxvt*background:              #fdf6e3
URxvt*foreground:              #657b83

! red
*color1:                       #a40000
*color9:                       #EF2929
! green
*color2:                       #4E9A06
*color10:                      #8AE234
! yellow
*color3:                       #C4A000
*color11:                      #FCE94F
! blue
*color4:                       #3465A4
*color12:                      #729FCF
! purple
*color5:                       #75507B
*color13:                      #AD7FA8
! orange                       (replaces cyan)
*color6:                       #ce5c00
*color14:                      #fcaf3e
! white
*color7:                       #babdb9
*color15:                      #EEEEEC
